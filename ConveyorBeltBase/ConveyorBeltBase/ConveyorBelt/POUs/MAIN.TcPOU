	qConveyor1 AT %QX0.0: BOOL; // First conveyor on/off
	
	// Elevator outputs
	qElevatorConveyor AT %QX1.0: BOOL; // Elevator's conveyor on/off
	qElevatorUp AT %QX1.1: BOOL; // Move elevator's table up
	qElevatorDown AT %QX1.2: BOOL; // Move elevator's table down
	
	// Conveyor 2 outputs
	qConveyor2 AT %QX2.0: BOOL; // Second conveyor on/off
	qPusherPush AT %QX2.1: BOOL; // Push with pusher
	qPusherRetract AT %QX2.2: BOOL; // Retract pusher
	
	// Box outputs
	qBoxShaker AT %QX3.0: BOOL; // Shake the box
END_VAR]]></Declaration>
